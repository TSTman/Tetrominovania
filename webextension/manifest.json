{
  "background": {
    "persistent": true,
    "scripts": [
      "scripts/background/request-listener.js"
    ]
  },
  "description": "Play tetromino.com games with less lag",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "96": "icons/96.png",
    "128": "icons/128.png"
  },
  "manifest_version": 2,
  "name": "Minimal tetromino.com",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://tetris.com/*"
  ],
  "version": "0.0.0"
}